{
    "version": 2,
    "builds": [
      {
        "src": "server.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",    
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/api/v1/(.*)",
        "destination": "server.js"
      }
    ]
  }